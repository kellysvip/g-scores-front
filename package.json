{
  "private": true,
  "description": "The backoffice frontend project",
  "scripts": {
    "lint": "eslint \"{components,pages,helpers,constants,config}/**/*.ts\"",
    "format": "prettier --write \"**/*.(ts|tsx|js|jsx)\"",
    "start:dev": "next",
    "build": "next build",
    "start": "next start",
    "jest": "NODE_ENV=test jest --config jest.config.js --no-compilation-cache",
    "test": "npm run lint && npm run jest",
    "test:cov": "NODE_ENV=test jest --config jest.config.js --coverage",
